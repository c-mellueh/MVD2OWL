								<Requirement applicability="export" requirement="mandatory" exchangeRequirement="c2768ec1-293d-4f4d-b806-92e8b60713a9"/>
								<Requirement applicability="import" requirement="mandatory" exchangeRequirement="c2768ec1-293d-4f4d-b806-92e8b60713a9"/>
							</Requirements>
							<TemplateRules operator="and">
								<TemplateRule Parameters="IsDecomposedBy[Exists]=TRUE AND IsDecomposedBy[Type]='IfcRelAggregates' AND RelatedObjects[Size]>=1 AND RelatedObjects[Type]='IfcDiscreteAccessory' AND PredefinedType[Value]='USERDEFINED' AND ObjectType[Value]='MOUNTINGPART'"/>
